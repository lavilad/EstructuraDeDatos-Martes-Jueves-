// Funci칩n para imprimir el 치rbol en orden de anchura
void printLevel(Node* root, int level) {
    if (root == NULL)
        return;
    if (level == 1)
        printf("%d ", root->key);
    else if (level > 1) {
        printLevel(root->left, level - 1);
        printLevel(root->right, level - 1);
    }


// Imprimir el 치rbol en orden de anchura
    printf("Imprimir el 치rbol en orden de anchura:\n");
    int h = height(root);
    for (int i = 1; i <= h; i++) {
        printf("Nivel %d: ", i);
        printLevel(root, i);
        printf("\n");
    }
